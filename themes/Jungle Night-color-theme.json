            ],
            "settings": {
                "foreground": "#80ffff"
            }
        },
        {
            "name": "URL",
            "scope": [
                "*link*",
                "*uri*",
                "*url*"
            ],
            "settings": {
                "fontStyle": "bold",
                "foreground": "#4545e5"
            }
        },
        {
            "name": "Decorators",
