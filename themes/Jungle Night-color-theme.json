    "name": "Jungle Night",
    "tokenColors": [
        {
            "name": "Comment",
            "scope": [
                "comment",
                "string.quoted.docstring"
            ],
            "settings": {
                "foreground": "#6D7073"
            }
        },
        {
            "name": "Builtins",
            "scope": [
                "support",
                "support.function.magic",
                "support.function.builtin"
            ],
            "settings": {
                "foreground": "#ffcc66"
            }
        },
        {
            "name": "Specials",
            "scope": [
                "variable.language.special",
                "variable.parameter.function.language.special"
            ],
            "settings": {
                "foreground": "#ff8080"
            }
        },
        {
            "name": "Variable",
            "scope": [
                "variable",
                "string.unquoted"
            ],
            "settings": {
                "foreground": "#dcdcdc"
            }
        },
        {
            "name": "Keyword, Storage",
            "scope": [
                "keyword",
                "storage"
            ],
            "settings": {
                "foreground": "#608060"
            }
        },
        {
            "name": "Operator, Punctuation",
            "scope": [
                "keyword.operator",
                "punctuation.separator",
                "punctuation.definition.parameters",
                "punctuation.definition.tag"
            ],
            "settings": {
                "foreground": "#ACAEAF"
            }
        },
        {
            "name": "Tag",
            "scope": [
                "entity.name.tag",
            ],
            "settings": {
                "foreground": "#608060"
            }
        },
        {
            "name": "Function, Special Method, Block Level",
            "scope": [
                "entity.name.function",
                "keyword.other.special-method",
                "meta.block-level",
                "meta.function-call",
                "support.function",
                "variable.function"
            ],
            "settings": {
                "foreground": "#ccbb88"
            }
        },
        {
            "name": "Other Variable, String Link",
            "scope": [
                "string.other.link",
                "support.other.variable"
            ],
            "settings": {
                "foreground": "#ff8080"
            }
        },
        {
            "name": "Number, Constant, Function Argument, Tag Attribute, Embedded",
            "scope": [
                "constant.character",
                "constant.language",
                "constant.numeric",
                "keyword.other.unit",
                "support.constant",
                "variable.parameter"
            ],
            "settings": {
                "foreground": "#cc8f52"
            }
        },
        {
            "name": "String, Symbols",
            "scope": [
                "constant.other.key",
                "constant.other.symbol",
                "string"
            ],
            "settings": {
                "foreground": "#8fbf8f"
            }
        },
        {
            "name": "Regular Expressions",
            "scope": [
                "string.regexp"
            ],
            "settings": {
                "foreground": "#ff8080"
            }
        },
        {
            "name": "Escape Characters",
            "scope": [
                "constant.character.escape"
            ],
            "settings": {
                "foreground": "#608060"
            }
        },
        {
            "name": "URL",
            "scope": [
                "*link*",
                "*uri*",
                "*url*"
            ],
            "settings": {
                "fontStyle": "bold",
                "foreground": "#8080ff"
            }
        },
            ],
    "type": "dark"
}